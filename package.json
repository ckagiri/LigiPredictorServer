{
  "name": "LigiPredictorServer",
  "version": "1.0.0",
  "description": "Ligi Predictor Server",
  "main": "index.js",
  "scripts": {
    "tsc:client": "tsc -p src/client",
    "tsc:server": "tsc -p src/server",
    "tsc:cs": "npm run tsc:client && npm run tsc:server",
    "mongodb": "mongod --dbpath=C:/data/db --port 27017",
    "start": "node src/server/app/server.js",
    "start:test": "concurrently \"tsc -p src/server\" \"tsc -p src/client\" && node src/server/app/server.js",
    "start:prod": "SET NODE_ENV=production&& gulp prod && concurrently  \"gulp tsc-watcher\" \"nodemon src/server/app/server.js\"",
    "start:dev": "SET NODE_ENV=development&& gulp dev && concurrently \"gulp tsc-watcher\" \"nodemon src/server/app/server.js\"",
    "test": "gulp test",
    "tsd:reinstall": "tsd reinstall -so",
    "db:dev:seeduser": "cross-env NODE_ENV=development ts-node src/server/db/tasks/seed-user.ts",
    "db:seed": "cross-env NODE_ENV=development node src/server/db/tasks/seed.js",
    "db:dropseed": "cross-env NODE_ENV=production node src/server/db/tasks/drop-seed.js",
    "db:test:dropseed": "cross-env NODE_ENV=test ts-node src/server/db/tasks/drop-seed.ts",
    "db:dev:dropseed": "cross-env NODE_ENV=development ts-node src/server/db/tasks/drop-seed.ts",
    "db:reset": "SET NODE_ENV=development&&node src/server/db/tasks/reset.js",
    "test:mocha": "mocha --reporter spec --compilers ts:ts-node/register src/**/*.spec.ts",
    "test:mocha:spec": "mocha --reporter spec --compilers ts:ts-node/register src/**/*.test.ts",
    "mocha": "node node_modules/mocha/bin/mocha  --reporter spec src/server/db/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckagiri/LigiPredictorServer.git"
  },
  "engines": {
    "node": "6.9.x",
    "npm": "3.10.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckagiri/LigiPredictorServer/issues"
  },
  "homepage": "https://github.com/ckagiri/LigiPredictorServer#readme",
